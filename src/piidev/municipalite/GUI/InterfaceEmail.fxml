      <Label fx:id="labelCode" alignment="CENTER" layoutX="585.0" layoutY="231.0" prefHeight="35.0" prefWidth="231.0" text="Tapez Votre Code">
         <font>
            <Font name="Arial" size="23.0" />
         </font>
      </Label>
      <TextField fx:id="codetextField" layoutX="585.0" layoutY="301.0" prefHeight="51.0" prefWidth="231.0" promptText="Votre code" />
      <Button fx:id="btn_valider" layoutX="728.0" layoutY="390.0" mnemonicParsing="false" onAction="#VerifCode" prefHeight="36.0" prefWidth="88.0" text="Valider" />
   </children>
</AnchorPane>
