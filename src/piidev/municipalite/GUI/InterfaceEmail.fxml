<AnchorPane id="Dashoard.css" prefHeight="652.0" prefWidth="787.0" styleClass="dash__side-bar" stylesheets="@Dashoard.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="piidev.municipalite.GUI.InterfaceEmailController">
   <children>
      <ImageView fitHeight="51.0" fitWidth="65.0" layoutX="28.0" layoutY="24.0" onMouseClicked="#goToLogin" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/back.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="192.0" fitWidth="231.0" layoutX="392.0" layoutY="39.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/header.jpg" />
         </image>
      </ImageView>
      <Label fx:id="labelEmail" alignment="CENTER" layoutX="89.0" layoutY="231.0" prefHeight="35.0" prefWidth="231.0" text="Tapez Votre Email">
         <font>
            <Font name="Arial" size="23.0" />
         </font>
      </Label>
      <TextField fx:id="emailTextField" layoutX="32.0" layoutY="301.0" prefHeight="51.0" prefWidth="385.0" promptText="Votre Email" />
      <Button fx:id="btn_verif" layoutX="303.0" layoutY="391.0" mnemonicParsing="false" onAction="#Verifier" prefHeight="25.0" prefWidth="114.0" text="Confirmer">
